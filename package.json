{
  "name": "transpileception",
  "version": "1.0.0",
  "description": "Transpiling JavaScript to transpile JavaScript to JavaScript.",
  "main": "index.js",
  "scripts": {
    "spin": "npm run xto6 && npm run babel && npm run rapydscript && npm run typescript && npm run coffee",
    "xto6": "xto6 src/person.js -o dist/person-es6.js",
    "babel": "babel dist/person-es6.js --out-file dist/person-es5.js",
    "rapydscript": "rapydscript src/person.rs -o dist/person-rapyd.js --prettify",
    "typescript": "tsc src/person.ts --out dist/person-ts.js",
    "coffee": "coffee --compile --output dist/ src/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jaydson/transpileception.git"
  },
  "author": "Jaydson Gomes <jayalemao@gmail.com> (http://jaydson.org/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jaydson/transpileception/issues"
  },
  "homepage": "https://github.com/jaydson/transpileception"
}
